{
  "manifest_version": 3,
  "name": "Custom Chrome Homepage",
  "version": "1.0",
  "description": "A personalized homepage with AI features.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
